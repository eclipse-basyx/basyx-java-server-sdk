          { "$strVal": "27-37-09-05" }
        ]
        }
      ]
      },
      { "$match": [
        {
          "$eq": [
            { "$field": "$sm#idShort" },
            { "$strVal": "TechnicalData" }
          ]
        },
        {
          "$eq": [
            { "$field": "$sme#semanticId" },
            { "$strVal": "0173-1#02-BAF016#006" }
          ]
        },
        {
